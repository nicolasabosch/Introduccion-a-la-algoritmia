    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="aportante1, aportante2, aportante3, total" type="Integer" array="False" size=""/>
            <output expression="&quot;Cuanto aporto el primero&quot;" newline="True"/>
            <input variable="aportante1"/>
            <output expression="&quot;Cuanto aporto el segundo&quot;" newline="True"/>
            <input variable="aportante2"/>
