            <output expression="&quot;El primero aporto el &quot;&amp;aportante1/total*100&amp;&quot;%&quot;" newline="True"/>
            <output expression="&quot;El segundo aporto el &quot;&amp;aportante2/total*100&amp;&quot;%&quot;" newline="True"/>
            <output expression="&quot;El tercero aporto el &quot;&amp;aportante3/total*100&amp;&quot;%&quot;" newline="True"/>
        </body>
    </function>
